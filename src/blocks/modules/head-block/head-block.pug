		.head-block__title-content
			p.head-block__epigraph Брендинг и ребрендинг
			h2.head-block__title Cтоматологические клиники и центры
		.head-block__picture
			picture
				source(media="(min-width: 1300px)", srcset="../img/content/interior-desktop@1x.png 1x, ../img/content/interior-desktop@1x.png 2x")
				source(type="image/webp" media="(min-width: 1300px)", srcset="../img/content/interior-desktop@1x.webp 1x, ../img/content/interior-desktop@1x.webp 2x")
				source(type="image/webp", srcset="../img/content/interior-mobile@1x.webp 1x, ../img/content/interior-mobile@2x.webp 2x")
